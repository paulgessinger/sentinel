<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>{% block title %}Sentinel{% endblock %}</title>
    {% block meta %}{% endblock %}
    <link rel="icon" type="image/png" sizes="32x32" href="{{ app.url_for('static', filename='sentinel_32p.png') }}">
    {% block styles %}
    <link rel="stylesheet" href="{{ app.url_for('static', filename='main.css') }}">
    {% endblock %}
    {% block head_extra %}{% endblock %}
</head>
<body{% block body_attrs %}{% endblock %}>
{% block body %}{% endblock %}
{% block scripts %}{% endblock %}
</body>
</html>
